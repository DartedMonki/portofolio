{
  "author": "DartedMonki",
  "name": "portofolio",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "dev:debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "lint": "next lint && tsc --noEmit",
    "lint:fix": "next lint --fix && tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "optimize-images": "node --import \"data:text/javascript,import { register } from 'node:module'; import { pathToFileURL } from 'node:url'; register('ts-node/esm', pathToFileURL('./'));\" scripts/optimize-images.mts",
    "build:with-images": "npm run optimize-images && next build"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "lodash": "^4.17.21",
    "next": "^14.2.23",
    "notistack": "^2.0.5",
    "react": "latest",
    "react-dom": "latest",
    "sharp": "^0.33.5",
    "swiper": "^11.2.2",
    "three": "^0.173.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/sharp": "^0.31.1",
    "@types/three": "^0.173.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "5.1.6"
  }
}
